{
  "name": "miyuri",
  "version": "0.1.1",
  "description": "this is an image component for react, preact and vue",
  "main": "dist/miyuri.js",
  "source": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShredderMing/miyuri"
  },
  "scripts": {
    "build": "npm-run-all --silent -p build:main build:react build:vue",
    "build:main": "microbundle",
    "build:react": "microbundle src/react -o react.js",
    "build:vue": "microbundle src/vue -o vue.js"
  },
  "keywords": [
    "preact",
    "preact-components",
    "react",
    "react-components",
    "vue",
    "vue-components",
    "image"
  ],
  "devDependencies": {
    "microbundle": "^0.4.4",
    "npm-run-all": "^4.1.2",
    "preact": "^8.2.7",
    "prop-types": "^15.6.1",
    "react": "^16.3.1"
  },
  "peerDependencies": {
    "react": "*",
    "preact": "*"
  }
}
